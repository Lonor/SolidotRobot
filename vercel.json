{
    "name": "solidot-robot",
    "version": 2,
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "consumerKey": "@consumerkey",
      "consumerSecret": "@consumersecret",
      "username": "@username",
      "password": "@password"
    },
    "routes": [
      { "src": "/api/.*", "dest": "/api/now.js" },
      { "src": "/.*", "dest": "/api/now.js" }
  ]
}